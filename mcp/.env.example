# os-openclaw-mcp environment variables

# API Gateway connection
OS_API_GATEWAY_URL=http://os-api-gateway-app/api/v1
OS_API_GATEWAY_KEY=<from-k8s-secret>

# Agent identity (used in X-User-Id header for all gateway calls)
AGENT_USER_ID=<system-agent-mongodb-objectid>
AGENT_USER_EMAIL=openclaw-agent@orchesight.internal

# Direct Triton access for high-volume ML batch operations
TRITON_GATEWAY_DIRECT_URL=http://triton-gateway.prod.svc.cluster.local:8080

# Server
PORT=3020
